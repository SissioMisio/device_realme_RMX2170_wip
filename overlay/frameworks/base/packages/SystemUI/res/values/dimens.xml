<resources>
    <!-- the padding on the start of the statusbar -->
    <dimen name="status_bar_padding_start">5px</dimen>

    <!-- the padding on the end of the statusbar -->
    <dimen name="status_bar_padding_end">16px</dimen>

   <!-- the padding on the top of the statusbar -->
    <dimen name="status_bar_padding_top">8dp</dimen>

    <!-- Starting padding for a left-aligned status bar clock -->
    <dimen name="status_bar_left_clock_starting_padding">6px</dimen>

    <!-- Height of the status bar header bar when on Keyguard -->
    <dimen name="status_bar_header_height_keyguard">@*android:dimen/status_bar_height</dimen>

    <!-- Margin on the left side of the carrier text on Keyguard -->
    <dimen name="keyguard_carrier_text_margin">@dimen/status_bar_padding_start</dimen>

    <!-- Margin on the right side of the system icon group on Keyguard. -->
    <dimen name="system_icons_keyguard_padding_end">0dp</dimen>

    <!-- The absolute side margins of quick settings -->
    <dimen name="rounded_corner_content_padding">8dp</dimen>

    <!-- Margin on the left side of the heads up notification -->
    <dimen name="heads_up_status_bar_padding_start">115px</dimen>

    <!-- Margin on the left side of the center clock -->
    <dimen name="status_bar_center_clock_padding_start">0px</dimen>

    <!-- Margin on the right side of the center clock -->
    <dimen name="status_bar_center_clock_padding_end">124px</dimen>

    <!-- The maximum count of notifications on Keyguard (only when ambient play indication is showing on keyguard).
         The rest will be collapsed in an overflow card.
     -->
    <integer name="keyguard_max_notification_count_ambient_play">2</integer>

    <!-- The bottom margin of the keyguard security container -->
    <dimen name="kg_security_container_min_bottom_margin">469px</dimen>

    <!-- Bottom margin size when fingerprint in display is active -->
    <dimen name="keyguard_indication_margin_bottom_fingerprint_in_display">@dimen/keyguard_indication_margin_bottom</dimen>

    <!-- Bottom margin size -->
    <dimen name="keyguard_indication_margin_bottom">300px</dimen>

    <!-- Location on the screen of the center of the physical volume up/down buttons. -->
    <dimen name="physical_volume_up_button_center_screen_location_y">750px</dimen>
    <dimen name="physical_volume_down_button_center_screen_location_y">1025px</dimen>
</resources>
